																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element ref="immissionLimitValue"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="sheet3b" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="critHorzTransDirection"/>
																			<xs:element ref="critVertTransDirection"/>
																			<xs:element ref="immissionLimitValue"/>
																			<xs:element ref="measHorzTransDirection"/>
																			<xs:element ref="measVertTransDirection"/>
																			<xs:element ref="measErpTransmittingPower"/>
																			<xs:element ref="measContrElecFieldIntensity"/>
																			<xs:element ref="newErpTransmittingPower"/>
