<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:element name="nisDataSpecification">
		<xs:annotation>
			<xs:documentation>3.2, 4.7.2014</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="filenameXml">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="256"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="filenamePdf" minOccurs="0">
					<xs:annotation>
						<xs:documentation>File name of the delivered file in the pdf format.
Obligatory if type = 01, 02, 11. (Checks 17, 18, 20)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="256"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="municipalityZipCode">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="4"/>
							<xs:maxLength value="12"/>
							<xs:pattern value="(([A]|[D]|[F]|[I])[\-][0-9][0-9][0-9][0-9]*)|([1-9][0-9][0-9][0-9])"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="municipality">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="projectKind" minOccurs="0">
					<xs:annotation>
						<xs:documentation>For example : new project. Obligatory if date &gt;= 1.6.2006.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="4"/>
							<xs:maxLength value="200"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="date" type="xs:date"/>
				<xs:element name="version" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="3"/>
							<xs:maxLength value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="codeRef" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reference ID of operator</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1"/>
							<xs:maxLength value="30"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="authorisation">
					<xs:annotation>
						<xs:documentation>01=Authorization request filed
02=Authorization granted
03=Put into service
04=Put out of service
05=Anlage erneut in Betrieb genommen
06=Authorization withdrawn
for microcells not necessary</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="01"/>
							<xs:enumeration value="02"/>
							<xs:enumeration value="03"/>
							<xs:enumeration value="04"/>
							<xs:enumeration value="05"/>
							<xs:enumeration value="06"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="type">
					<xs:annotation>
						<xs:documentation>01=Normal with PDF and details
02=Normal with PDF 03=Normal with details
11=Micro with PDF 12=Micro</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="01"/>
							<xs:enumeration value="02"/>
							<xs:enumeration value="03"/>
							<xs:enumeration value="11"/>
							<xs:enumeration value="12"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="stationLocation">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="address"/>
							<xs:element ref="zipCode"/>
							<xs:element ref="location"/>
							<xs:element ref="coordinateEast"/>
							<xs:element ref="coordinateNorth"/>
							<xs:element ref="coordinateHigh"/>
							<xs:element name="lotNumber" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="50"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="description" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Obligatory if date &gt;= 1.6.2006</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:maxLength value="256"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="region" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="regionLocation" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Obligatory if date &gt;= 1.6.2006</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="name" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:NMTOKEN">
												<xs:enumeration value="AG"/>
												<xs:enumeration value="AI"/>
												<xs:enumeration value="AR"/>
												<xs:enumeration value="BE"/>
												<xs:enumeration value="BL"/>
												<xs:enumeration value="BS"/>
												<xs:enumeration value="FR"/>
												<xs:enumeration value="GE"/>
												<xs:enumeration value="GL"/>
												<xs:enumeration value="GR"/>
												<xs:enumeration value="JU"/>
												<xs:enumeration value="LU"/>
												<xs:enumeration value="NE"/>
												<xs:enumeration value="NW"/>
												<xs:enumeration value="OW"/>
												<xs:enumeration value="SG"/>
												<xs:enumeration value="SH"/>
												<xs:enumeration value="SO"/>
												<xs:enumeration value="SZ"/>
												<xs:enumeration value="TG"/>
												<xs:enumeration value="TI"/>
												<xs:enumeration value="UR"/>
												<xs:enumeration value="VD"/>
												<xs:enumeration value="VS"/>
												<xs:enumeration value="ZG"/>
												<xs:enumeration value="ZH"/>
												<xs:enumeration value="FL"/>
												<xs:enumeration value="AUT"/>
												<xs:enumeration value="D"/>
												<xs:enumeration value="F"/>
												<xs:enumeration value="I"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="role" use="required">
										<xs:annotation>
											<xs:documentation>1=Region 2=Neighbouring region</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:NMTOKEN">
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="contact" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Obligatory if date &gt;= 1.6.2006</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="firstname" minOccurs="0"/>
							<xs:element ref="lastname"/>
							<xs:element ref="address"/>
							<xs:element ref="zipCode"/>
							<xs:element ref="location"/>
							<xs:element ref="phone"/>
							<xs:element ref="fax" minOccurs="0"/>
							<xs:element ref="email" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="admission" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Obligatory if date &gt;= 1.6.2006, but for microcells not necessary</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="firstname" minOccurs="0"/>
							<xs:element ref="lastname"/>
							<xs:element ref="address"/>
							<xs:element ref="zipCode"/>
							<xs:element ref="location"/>
							<xs:element ref="phone"/>
							<xs:element ref="fax" minOccurs="0"/>
							<xs:element ref="email" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="station" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="operatorCode">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Swisscom"/>
										<xs:enumeration value="Orange"/>
										<xs:enumeration value="Salt"/>
										<xs:enumeration value="Sunrise"/>
										<xs:enumeration value="Tele2"/>
										<xs:enumeration value="InPhone"/>
										<xs:enumeration value="SBB"/>
										<xs:enumeration value="Foreign"/>
										<xs:enumeration value="PMR"/>
										<xs:enumeration value="Polycom"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="stationCode">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="15"/>
										<xs:pattern value=""/>
										<xs:pattern value="((([A-Z]|[a-z]|[0-9])+)((([\-]|[\i]|[\.]|[\s])?)(([A-Z]|[a-z]|[0-9])+))*)"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="stationRemark" minOccurs="0">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:minLength value="1"/>
										<xs:maxLength value="255"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="role">
								<xs:annotation>
									<xs:documentation>F=Lead management
M=Participation with signature</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="F"/>
										<xs:enumeration value="M"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="cellElement" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="antennaNumber">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="7"/>
													<xs:pattern value="((([A-Z]|[a-z]|[0-9]|[\-])+)((([\-]|[\i]|[\.]|[\s])?)(([A-Z]|[a-z]|[0-9])+))*)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="serviceType">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="GSM900"/>
													<xs:enumeration value="GSM0900"/>
													<xs:enumeration value="GSM1800"/>
													<xs:enumeration value="UMTS"/>
													<xs:enumeration value="UMTS900"/>
													<xs:enumeration value="UMTS0900"/>
													<xs:enumeration value="UMTS1800"/>
													<xs:enumeration value="UMTS2100"/>
													<xs:enumeration value="UMTS2600"/>
													<xs:enumeration value="GSM-Rail"/>
													<xs:enumeration value="Tetra"/>
													<xs:enumeration value="Tetrapol"/>
													<xs:enumeration value="800"/>
													<xs:enumeration value="0800"/>
													<xs:enumeration value="900"/>
													<xs:enumeration value="0900"/>
													<xs:enumeration value="1800"/>
													<xs:enumeration value="2100"/>
													<xs:enumeration value="2600"/>
													<xs:enumeration value="800-900"/>
													<xs:enumeration value="0800-0900"/>
													<xs:enumeration value="1800-2100"/>
													<xs:enumeration value="2100-2600"/>
													<xs:enumeration value="1800-2600"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="sequentialNumber" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Obligatory if date &gt;= 1.6.2006, not necessary if Type = 11 or 12, Check 24</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:int">
													<xs:totalDigits value="5"/>
													<xs:fractionDigits value="0"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="coordinateEast" minOccurs="0">
											<xs:annotation>
												<xs:documentation>relative coordinates.Obligatory if date &gt;= 1.6.2006, not necessary if type = 11 or 12</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-9999.99"/>
													<xs:maxInclusive value="9999.99"/>
													<xs:totalDigits value="7"/>
													<xs:fractionDigits value="2"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="coordinateNorth" minOccurs="0">
											<xs:annotation>
												<xs:documentation>relative coordinates. Obligatory if date &gt;= 1.6.2006, not necessary if type = 11 or 12</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-9999.99"/>
													<xs:maxInclusive value="9999.99"/>
													<xs:totalDigits value="7"/>
													<xs:fractionDigits value="2"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="coordinateHigh" minOccurs="0">
											<xs:annotation>
												<xs:documentation>relative coordinates. Obligatory if date &gt;= 1.6.2006, not necessary if type = 11 or 12</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:decimal">
													<xs:minInclusive value="-5000"/>
													<xs:maxInclusive value="5000"/>
													<xs:totalDigits value="7"/>
													<xs:fractionDigits value="2"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element ref="erpTransmittingPower"/>
										<xs:element name="cellElementRefSumPower" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1"/>
													<xs:maxLength value="7"/>
													<xs:pattern value="((([A-Z]|[a-z]|[0-9]|[\-])+)((([\-]|[\i]|[\.]|[\s])?)(([A-Z]|[a-z]|[0-9])+))*)"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="azimuthOf">
											<xs:simpleType>
												<xs:restriction base="xs:int">
													<xs:totalDigits value="3"/>
													<xs:fractionDigits value="0"/>
													<xs:minInclusive value="0"/>
													<xs:maxInclusive value="359"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="azimuthTo" minOccurs="0">
											<xs:simpleType>
												<xs:restriction base="xs:int">
													<xs:totalDigits value="3"/>
													<xs:fractionDigits value="0"/>
													<xs:minInclusive value="0"/>
													<xs:maxInclusive value="359"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="sheet1" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="type">
														<xs:annotation>
															<xs:documentation>S=Antenna on the same mast
P=Antenna located in the perimeter of the installation</xs:documentation>
														</xs:annotation>
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="S"/>
																<xs:enumeration value="P"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="location" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="1"/>
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="sheet2" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="typeAntenna">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:minLength value="1"/>
																<xs:maxLength value="50"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="mechanicalTiltOf" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
