																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="mechanicalTiltTo" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
																<xs:minInclusive value="-90"/>
																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="electricalTiltOf" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
																<xs:minInclusive value="-90"/>
																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="electricalTiltTo" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
																<xs:minInclusive value="-90"/>
																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="totalTiltOf">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
																<xs:minInclusive value="-90"/>
																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:element name="totalTiltTo" minOccurs="0">
														<xs:simpleType>
															<xs:restriction base="xs:int">
																<xs:totalDigits value="3"/>
																<xs:fractionDigits value="0"/>
																<xs:minInclusive value="-90"/>
																<xs:maxInclusive value="+90"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="locations" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="location" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="locationNr"/>
																<xs:element ref="azimuthToAntenna"/>
																<xs:element ref="elevationToAntenna"/>
																<xs:element name="sheet3a" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="critHorzTransDirection"/>
																			<xs:element ref="critVertTransDirection"/>
																			<xs:element name="fieldIntensity">
																				<xs:simpleType>
