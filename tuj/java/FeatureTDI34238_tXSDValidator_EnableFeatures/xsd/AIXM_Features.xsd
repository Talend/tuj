					<xsd:complexType >
						<xsd:simpleContent >
							<xsd:extension base="aixm:CodeTimeEventCombinationType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
