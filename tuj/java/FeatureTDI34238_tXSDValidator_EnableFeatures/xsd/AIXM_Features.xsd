						<xsd:simpleContent >
							<xsd:extension base="aixm:ValAngleType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="slopeAccuracy" nillable="true" minOccurs="0" >
