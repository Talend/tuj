			<xsd:sequence >
				<xsd:group ref="aixm:ServicePropertyGroup" minOccurs="0"  />
				<xsd:element name="type" nillable="true" minOccurs="0" >
					<xsd:annotation>
						<xsd:appinfo>AIXM 4.5</xsd:appinfo>
						<xsd:documentation>The type of information service provided.
</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType >
						<xsd:simpleContent >
							<xsd:extension base="aixm:CodeServiceInformationType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="voice" nillable="true" minOccurs="0" >
					<xsd:annotation>
						<xsd:appinfo>AIXM 4.5</xsd:appinfo>
						<xsd:documentation>Indicating whether the service is provided as voice message.
</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType >
						<xsd:simpleContent >
							<xsd:extension base="aixm:CodeYesNoType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="dataLink" nillable="true" minOccurs="0" >
					<xsd:annotation>
						<xsd:documentation>Indicates whether the service is available via data-link.</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType >
						<xsd:simpleContent >
							<xsd:extension base="aixm:CodeYesNoType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="navaidBroadcast" type="aixm:VORPropertyType"  minOccurs="0"  maxOccurs="unbounded" nillable="true" >
