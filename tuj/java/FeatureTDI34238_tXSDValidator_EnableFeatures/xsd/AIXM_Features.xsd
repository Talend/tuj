								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="reflector"  minOccurs="0"  maxOccurs="unbounded" nillable="true" >
					<xsd:annotation>
						<xsd:documentation>Identifies the Reflectors utilized with a PAR.</xsd:documentation>
