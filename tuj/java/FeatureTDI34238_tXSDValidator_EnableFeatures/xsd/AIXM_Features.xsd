						<xsd:element name="extension" minOccurs="0" maxOccurs="unbounded" >
							<xsd:complexType >
								<xsd:choice >
									<xsd:element ref="aixm:AbstractPrecisionApproachRadarExtension"  />
									<xsd:element ref="aixm:AbstractRadarEquipmentExtension"  />
								</xsd:choice>
								<xsd:attributeGroup ref="gml:OwnershipAttributeGroup"  />
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
		<xsd:group name="PrecisionApproachRadarPropertyGroup" >
			<xsd:sequence >
				<xsd:group ref="aixm:RadarEquipmentPropertyGroup" minOccurs="0"  />
