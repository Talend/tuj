								<xsd:choice >
									<xsd:element ref="aixm:AbstractGuidanceLineLightSystemExtension"  />
									<xsd:element ref="aixm:AbstractGroundLightSystemExtension"  />
								</xsd:choice>
								<xsd:attributeGroup ref="gml:OwnershipAttributeGroup"  />
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
