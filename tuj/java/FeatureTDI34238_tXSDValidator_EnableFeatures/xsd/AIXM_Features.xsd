				<xsd:element name="clientAirspace" type="aixm:AirspacePropertyType"  minOccurs="0"  maxOccurs="unbounded" nillable="true" >
				</xsd:element>
				<xsd:element name="clientAirport" type="aixm:AirportHeliportPropertyType"  minOccurs="0"  maxOccurs="unbounded" nillable="true" >
				</xsd:element>
				<xsd:element name="clientRoute"  minOccurs="0"  maxOccurs="unbounded" nillable="true" >
					<xsd:complexType >
						<xsd:complexContent >
							<xsd:extension base="aixm:RoutePortionPropertyType" >
								<xsd:attribute name="nilReason" type="gml:NilReasonEnumeration"  />
							</xsd:extension>
