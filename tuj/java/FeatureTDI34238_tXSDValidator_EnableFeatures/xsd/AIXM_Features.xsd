		<xsd:group name="TouchDownLiftOffLightSystemPropertyGroup" >
			<xsd:sequence >
				<xsd:group ref="aixm:GroundLightSystemPropertyGroup" minOccurs="0"  />
				<xsd:element name="position" nillable="true" minOccurs="0" >
					<xsd:annotation>
						<xsd:appinfo>AIXM 4.5</xsd:appinfo>
						<xsd:documentation>A code indicating the part of the TLOF surface the lighting system is serving.
For example, TDZ, THR, center line, edge, etc..</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType >
						<xsd:simpleContent >
							<xsd:extension base="aixm:CodeTLOFSectionType" >
