		<xsd:group name="GuidanceLineLightSystemPropertyGroup" >
			<xsd:sequence >
				<xsd:group ref="aixm:GroundLightSystemPropertyGroup" minOccurs="0"  />
				<xsd:element name="lightedGuidanceLine" type="aixm:GuidanceLinePropertyType"  minOccurs="0"  maxOccurs="1" nillable="true" >
					<xsd:annotation>
						<xsd:documentation>Identifies the Guidance Line for which the Lighting System is provided.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
