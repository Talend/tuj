			</xsd:sequence>
			<xsd:attributeGroup ref="gml:OwnershipAttributeGroup"  />
		</xsd:complexType>
		<xsd:element name="GuidanceLineLightSystemTimeSlice" type="aixm:GuidanceLineLightSystemTimeSliceType" substitutionGroup="gml:AbstractTimeSlice"  />
		<xsd:complexType name="GuidanceLineLightSystemTimeSliceType" >
			<xsd:complexContent >
				<xsd:extension base="aixm:AbstractAIXMTimeSliceType" >
					<xsd:sequence  >
						<xsd:group ref="aixm:GuidanceLineLightSystemPropertyGroup"  />
