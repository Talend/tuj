					</xsd:annotation>
					<xsd:complexType >
						<xsd:complexContent >
							<xsd:extension base="aixm:ValFrequencyType" >
								<xsd:attribute name="nilReason"  type="gml:NilReasonEnumeration"   />
							</xsd:extension>
						</xsd:complexContent>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="frequencyReception" nillable="true" minOccurs="0" >
