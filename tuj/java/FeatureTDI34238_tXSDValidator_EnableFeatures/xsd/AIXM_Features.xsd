				<xsd:element name="lightedTouchDownLiftOff" type="aixm:TouchDownLiftOffPropertyType"  minOccurs="0"  maxOccurs="1" nillable="true" >
					<xsd:annotation>
						<xsd:documentation>Identifies the Touch Down Lift Off area for which the Lighting System is provided.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:group>
		<xsd:element name="AbstractTouchDownLiftOffLightSystemExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"  />
		<xsd:complexType name="GuidanceLineLightSystemPropertyType" >
