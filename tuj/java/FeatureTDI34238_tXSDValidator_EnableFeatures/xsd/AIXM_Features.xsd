							<xsd:complexType >
								<xsd:choice >
									<xsd:element ref="aixm:AbstractInformationServiceExtension"  />
									<xsd:element ref="aixm:AbstractServiceExtension"  />
								</xsd:choice>
								<xsd:attributeGroup ref="gml:OwnershipAttributeGroup"  />
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
